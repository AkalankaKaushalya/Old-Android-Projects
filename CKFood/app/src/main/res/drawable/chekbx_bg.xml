<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:width="2dp"
                android:color="#0AEC9C"/>
            <gradient android:color="#00FFFFFF" />
            <corners
                android:radius="15dp"/>
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp">
            </padding>
        </shape>
    </item>
</layer-list>